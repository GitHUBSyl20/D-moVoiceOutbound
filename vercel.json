{
  "rewrites": [
    {
      "source": "/api/debug",
      "destination": "/api/debug"
    },
    {
      "source": "/test-root",
      "destination": "/api/test-root"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

